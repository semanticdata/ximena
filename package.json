{
    "name": "ximena",
    "version": "1.1.0",
    "description": "Ximena's description.",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel clean dev:*",
        "build": "npx @11ty/eleventy",
        "check": "npx prettier . --check",
        "format": "npx prettier . --write",
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rimraf _site",
        "dev:start": "npx @11ty/eleventy --serve --quiet",
        "dev:tailwindcss": "npx tailwindcss -i ./public/assets/css/tailwind.css -o ./_site/assets/css/tailwindy.css --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/semanticdata/ximena.git"
    },
    "keywords": [
        "eleventy",
        "11ty",
        "sass",
        "tailwindcss",
        "postcss"
    ],
    "author": "Miguel Pimentel",
    "homepage": "https://miguelpimentel.do/",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "@tailwindcss/typography": "^0.5.10",
        "autoprefixer": "^10.4.18",
        "cssnano": "^6.1.0",
        "daisyui": "^4.7.3",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1"
    },
    "dependencies": {
        "eleventy-sass": "^2.2.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5"
    }
}
